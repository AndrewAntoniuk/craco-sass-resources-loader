{
  "name": "craco-sass-loader",
  "version": "1.0.2",
  "description": "A craco plugin to use sass-resources-loader with create-react-app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndrewAntoniuk/craco-sass-resources-loader.git"
  },
  "main": "index.js",
  "author": "Julien Tilap LA VINH, Scott Fischer, Andrii Antoniuk",
  "license": "MIT",
  "keywords": [
    "craco",
    "create-react-app",
    "react",
    "webpack",
    "sass",
    "sass-resources-loader"
  ],
  "devDependencies": {
    "commitlint": "^17.1.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "sass-resources-loader": "^2.2.4"
  },
  "husky": {
